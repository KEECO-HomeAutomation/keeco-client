{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["prettier", "react"],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"prettier/prettier": "error"
	}
}
