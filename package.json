{
  "name": "keeco-client",
  "version": "0.1.0",
  "description": "Web based client to access and manage the nodes connected to the keeco-hub",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:coverage": "jest --collectCoverage --verbose",
    "lint": "eslint 'src/**/*.js' 'src/**/*.jsx' 'scripts/**/*.js'",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.graphql' 'src/**/*.html' 'scripts/**/*.js'",
    "start": "webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack",
    "graphql:generate": "node scripts/graphqlGenerate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KEECO-HomeAutomation/keeco-client.git"
  },
  "keywords": [
    "home",
    "automation",
    "client"
  ],
  "author": "KEECO",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/KEECO-HomeAutomation/keeco-client/issues"
  },
  "homepage": "https://github.com/KEECO-HomeAutomation/keeco-client#readme",
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/lab": "^4.0.0-alpha.15",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "apollo-utilities": "^1.2.1",
    "classnames": "^2.2.6",
    "formik": "^1.5.7",
    "graphql": "^14.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import-graphql": "^2.7.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "path": "^0.12.7",
    "prettier": "^1.17.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  }
}
